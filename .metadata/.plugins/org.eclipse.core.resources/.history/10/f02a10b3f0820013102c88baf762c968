#include<climits>
int divide(int dividend, int divisor) {

        long long dvd = dividend;
        long long div = divisor;
        bool sign = ( dvd >= 0 && div >= 0 ) || (dvd <= 0 && div <= 0) ? true : false;
        dvd = sign ? dvd : -dvd;
        div = sign ? div : -div;

        if(div == 0) return sign ? INT_MAX : INT_MIN;

        long long Q = 0;

        while(dvd)
        {
            int i = -1;
            while(dvd >= div<<(i+1))
            {
                i++;
            }
            Q |= i == -1 ? 0 : 1<<i;
            dvd -= i == -1 ? 0 : div<<i;
        }

        if((int) (Q - 1) == INT_MAX)
        {
            return sign ? INT_MAX : INT_MIN;
        }

        return sign ? Q : -Q;
    }
